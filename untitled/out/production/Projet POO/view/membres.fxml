<AnchorPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.MembresController">

    <TableView fx:id="tableMembres" layoutX="20" layoutY="20" prefWidth="400" prefHeight="300">
        <columns>
            <TableColumn fx:id="colNom" text="Nom"/>
            <TableColumn fx:id="colEmail" text="Email"/>
            <TableColumn fx:id="colTel" text="Téléphone"/>
        </columns>
    </TableView>

    <TextField fx:id="tfNom" layoutX="450" layoutY="20" promptText="Nom"/>
    <TextField fx:id="tfEmail" layoutX="450" layoutY="60" promptText="Email"/>
    <TextField fx:id="tfTelephone" layoutX="450" layoutY="100" promptText="Téléphone"/>

    <Button text="Ajouter" layoutX="450" layoutY="150" onAction="#ajouterMembre"/>
    <Button text="Modifier" layoutX="540" layoutY="150" onAction="#modifierMembre"/>
    <Button text="Supprimer" layoutX="630" layoutY="150" onAction="#supprimerMembre"/>

</AnchorPane>
